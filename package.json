  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "stg": "rm -rf ./.cache && yarn && GATSBY_ACTIVE_ENV=staging gatsby develop -H 0.0.0.0",
    "develop-stg": "yarn stg",
    "prod": "rm -rf ./.cache && yarn && GATSBY_ACTIVE_ENV=production gatsby develop -H 0.0.0.0",
    "develop-prod": "yarn prod",
    "deploy": "gatsby-plugin-s3 deploy",
    "format": "prettier --write src/**/*.{js,jsx}",
    "prebuild": "yarn clean && yarn",
    "deploy-stg": "GATSBY_ACTIVE_ENV=staging gatsby build && yarn deploy",
    "deploy-prod": "GATSBY_ACTIVE_ENV=production gatsby build && yarn deploy",
    "start": "yarn develop"
  }
